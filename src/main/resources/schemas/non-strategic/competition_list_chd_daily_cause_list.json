{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Root",
  "type": "array",
  "items": {
    "title": "Hearing list",
    "type": "object",
    "required": [
      "judge",
      "time",
      "venue",
      "type",
      "caseNumber",
      "caseName",
      "additionalInformation"
    ],
    "properties": {
      "judge": {
        "title": "Judge name",
        "type": "string",
        "default": "",
        "examples": [
          "Judge name"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "time": {
        "title": "Time of hearing",
        "type": "string",
        "default": "",
        "examples": [
          "10:30am"
        ],
        "pattern": "^\\d{1,2}([:.]\\d{2})?[ap]m$"
      },
      "venue": {
        "title": "Venue name",
        "type": "string",
        "default": "",
        "examples": [
          "This is venue name"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "type": {
        "title": "Type of hearing",
        "type": "string",
        "default": "",
        "examples": [
          "This is hearing type"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "caseNumber": {
        "title": "Case reference number",
        "type": "string",
        "default": "",
        "examples": [
          "1234"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "caseName": {
        "title": "Case name",
        "type": "string",
        "default": "",
        "examples": [
          "This is case name"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "hearingType": {
        "title": "Type of hearing being presented",
        "type": "string",
        "default": "",
        "examples": [
          "mda"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "additionalInformation": {
        "title": "Additional information",
        "type": "string",
        "default": "",
        "examples": [
          "This is additional information"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      }
    }
  }
}
