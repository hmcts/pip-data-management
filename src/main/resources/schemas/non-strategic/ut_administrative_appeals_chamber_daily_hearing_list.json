{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Root",
  "type": "array",
  "items": {
    "title": "Hearing list",
    "type": "object",
    "required": [
      "time",
      "appellant",
      "caseReferenceNumber",
      "judges",
      "members",
      "modeOfHearing",
      "venue",
      "additionalInformation"
    ],
    "properties": {
      "time": {
        "title": "Time",
        "type": "string",
        "default": "",
        "examples": [
          "10am"
        ],
        "pattern": "^\\d{1,2}([:.]\\d{2})?[ap]m$"
      },
      "appellant": {
        "title": "Appellant",
        "type": "string",
        "default": "",
        "examples": [
          "Appellant 1"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "caseReferenceNumber": {
        "title": "Case reference number",
        "type": "string",
        "default": "",
        "examples": [
          "12345"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "judges": {
        "title": "Judges",
        "type": "string",
        "default": "",
        "examples": [
          "Judge 1"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "members": {
        "title": "Members",
        "type": "string",
        "default": "",
        "examples": [
          "Member 1"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "modeOfHearing": {
        "title": "Mode of Hearing",
        "type": "string",
        "default": "",
        "examples": [
          "Hearing mode 1"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "venue": {
        "title": "Venue",
        "type": "string",
        "default": "",
        "examples": [
          "Venue 1"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      },
      "additionalInformation": {
        "title": "Additional information",
        "type": "string",
        "default": "",
        "examples": [
          "This is additional information"
        ],
        "pattern": "^(?!(.|\\r|\\n)*<[^>]+>)(.|\\r|\\n)*$"
      }
    }
  }
}
